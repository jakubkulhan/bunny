{
    "name": "bunny/bunny",
    "type": "library",
    "description": "Performant pure-PHP AMQP (RabbitMQ) sync/async (ReactPHP) library",
    "keywords": [
        "rabbitmq",
        "rabbit",
        "bunny",
        "amqp",
        "messaging",
        "message",
        "queue",
        "queueing",
        "exchange",
        "react",
        "react-php",
        "reactphp"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Kulhan",
            "email": "jakub.kulhan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1 <8.2",
        "react/event-loop": "^1.0 || ^0.5 || ^0.4",
        "react/promise": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Bunny\\": "src/Bunny/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bunny\\": "test/Bunny/",
            "Bunny\\Test\\Library\\": "test/Library/"
        }
    }
}
