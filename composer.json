{
    "name": "bunny/bunny",
    "type": "library",
    "description": "Performant pure-PHP AMQP (RabbitMQ) sync/async (ReactPHP) library",
    "keywords": [
        "rabbitmq",
        "rabbit",
        "bunny",
        "amqp",
        "messaging",
        "message",
        "queue",
        "queueing",
        "exchange",
        "react",
        "react-php",
        "reactphp"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Kulhan",
            "email": "jakub.kulhan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "psr/log": "~1.0",
        "react/event-loop": "~0.4",
        "react/promise": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Bunny\\": [
                "src/Bunny/",
                "test/Bunny/"
            ]
        },
        "classmap": [
            "src/Bunny/",
            "test/Bunny/"
        ]
    }
}
